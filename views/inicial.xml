<odoo>
    <data>
        <record model="ir.actions.act_window" id="inicial_list_action">
            <field name="name">A acci√≥n Inicial</field><!--O que se visualiza na parte superior esquerda-->
            <field name="res_model">odoo_basico.inicial</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <img src="odoo_basico/static/importarImaxes/odoohero.jpg" alt="imaxe inicial"  width="600px"/>
            </field>
        </record>

        <record model="ir.ui.view" id="inicial_tree_view">
